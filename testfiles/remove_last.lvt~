\input regression-test.tex
\documentclass{article}
\usepackage{erw-l3}
\ExplSyntaxOn

\ExplSyntaxOff
\begin{document}
\ExplSyntaxOn
\START

%----

\bool_set:Nn\l_tmpa_bool
{\exp_args:Ne\tl_if_empty_p:n
  {\erw_remove_last:n{}}}

\TEST{ \erw_remove_last:n{{foo}} == {} }
{%
  \bool_if:NTF\l_tmpa_bool\TRUE\FALSE
}

%----

\bool_set:Nn
\l_tmpa_bool
{
  \exp_args:Ne
  \str_if_eq_p:nn
  { \erw_remove_last:n{{foo}{bar}} }
  {{foo}}
}

\TEST{ \erw_remove_last:n{{foo}{bar}} == {{foo}} }
{%
  \bool_if:NTF
  \l_tmpa_bool \TRUE\FALSE}

%----

\bool_set:Nn
\l_tmpa_bool
{
  \exp_args:Ne
  \str_if_eq_p:nn
  { \erw_remove_last:n{{foo}{bar}{qux}} }
  {{foo}{bar}}
}

\TEST{ \erw_remove_last:n{{foo}{bar}{qux}} == {{foo}{bar}} }
{%
  \bool_if:NTF
  \l_tmpa_bool \TRUE\FALSE}

%----

\bool_set:Nn
\l_tmpa_bool
{
  \exp_args:Ne
  \str_if_eq_p:nn
  { \erw_remove_last:n{nnnN} }
  {nnn}
}

\TEST{ \erw_remove_last:n{ nnnN } == nnn }
{%
  \bool_if:NTF
  \l_tmpa_bool \TRUE\FALSE}

%\END
\ExplSyntaxOff
\end{document}