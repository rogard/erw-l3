\input regression-test.tex
\documentclass{article}
\usepackage{erw-l3}
\ExplSyntaxOn

\ExplSyntaxOff
\begin{document}
\ExplSyntaxOn
\START

\bool_set:Nn\l_tmpa_bool
{
  \exp_args:Ne
  \str_if_eq_p:nn
  { \erw_signature:n{#1{#3{#2}}{#4}#2} }
  { NnNn }
}

\TEST{ \erw_signature:n{#1{#3{#2}}{#4}#2} == NnNn }
{ \bool_if:NTF\TRUE\FALSE }

%\END
\ExplSyntaxOff
\end{document}