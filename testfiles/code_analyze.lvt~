\input regression-test.tex
\documentclass{article}
\usepackage{erw-l3}
\ExplSyntaxOn

\ExplSyntaxOff
\begin{document}
\ExplSyntaxOn
\START

\bool_set:Nn
\l_tmpa_bool
{%
  \exp_args:Ne
  \str_if_eq_p:nn
  {%
    \erw_code_analyze:n{#1{#3{#2}}#4{#2}}
  }
  {{1=N}{2=n}{3=N}{4=N}}
}

\TEST{ \erw_code_analyze:n{#1{#3{#2}}#4{#2}} == {{1=N}{3=N}{2=n}{4=N}} }
{ \bool_if:NTF\l_tmpa_bool\TRUE\FALSE }

%\END
\ExplSyntaxOff
\end{document}