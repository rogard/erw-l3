\START

\bool_set:Nn
\l_tmpa_bool
{ \erw_key_compare_p:n{} }
\TEST
{ \erw_key_compare_p:n{} == TRUE }
{ \bool_if:NTF\l_tmpa_bool\TRUE\FALSE }

\bool_set:Nn
\l_tmpa_bool
{ \erw_key_compare_p:n{ { 1 = x } } }
\TEST
{ \erw_key_compare_p:n{ { 1 = x } } == TRUE }
{ \bool_if:NTF\l_tmpa_bool\TRUE\FALSE }

\bool_set:Nn
\l_tmpa_bool
{ \erw_key_compare_p:n{ { 1 = x }{ 2 = y } } }
\TEST
{ \erw_key_compare_p:n{ { 1 = x }{ 2 = y } } == TRUE }
{ \bool_if:NTF\l_tmpa_bool\TRUE\FALSE }

\bool_set:Nn
\l_tmpa_bool
{ \erw_key_compare_p:n{ { 1 = x }{ 2 = y }{ 3 = z } } }
\TEST
{ \erw_key_compare_p:n{ { 1 = x }{ 2 = y }{ 3 = z } } == TRUE }
{ \bool_if:NTF\l_tmpa_bool\TRUE\FALSE }

\bool_set:Nn
\l_tmpa_bool
{ \erw_key_compare_p:n{ { 2 = x }{ 1 = y }{ 3 = z } } }
\TEST
{ \erw_key_compare_p:n{ { 2 = x }{ 1 = y }{ 3 = z } } == FALSE }
{ \bool_if:NTF\l_tmpa_bool\FALSE\TRUE }

\bool_set:Nn
\l_tmpa_bool
{ \erw_key_compare_p:n{ { 1 = x }{ 3 = y }{ 2 = z } } }
\TEST
{ \erw_key_compare_p:n{ { 1 = x }{ 3 = y }{ 2 = z } } == FALSE }
{ \bool_if:NTF\l_tmpa_bool\FALSE\TRUE }

